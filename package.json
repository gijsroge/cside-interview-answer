{
  "name": ".",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3000 \" \"pnpm relay --watch\"",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "typecheck": "tsc --noEmit",
    "relay": "mkdirp ./src/utils/relay/__generated__ && relay-compiler"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-pacer": "^0.19.3",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.151.1",
    "@tanstack/react-router-devtools": "^1.151.1",
    "@tanstack/router-plugin": "^1.151.1",
    "@tanstack/zod-adapter": "^1.151.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "nuqs": "^2.8.6",
    "react": "^19.0.3",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.0.3",
    "react-relay": "^20.1.1",
    "relay-runtime": "^20.1.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "unplugin-icons": "^23.0.1",
    "use-go-back": "^1.0.0",
    "vite-plugin-relay": "^2.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@iconify-json/tabler": "^1.2.26",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^20.1.1",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "jsdom": "^27.4.0",
    "mkdirp": "^3.0.1",
    "relay-compiler": "^20.1.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "web-vitals": "^5.1.0"
  },
  "relay": {
    "src": "./src/",
    "schema": "./github.graphql",
    "language": "typescript",
    "excludes": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "artifactDirectory": "./src/utils/relay/__generated__",
    "noFutureProofEnums": true,
    "eagerEsModules": true
  },
  "packageManager": "pnpm@10.28.0"
}
